{
    "name": "Print Google Maps Better",
    "description": "Print Google Maps Better!",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.google.com/maps/*"],
            "js": ["printer.js"]
        }
    ],
    "host_permissions": ["https://www.google.com/maps/*"]
}
